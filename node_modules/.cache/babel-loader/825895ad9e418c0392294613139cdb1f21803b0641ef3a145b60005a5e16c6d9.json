r.author }} | 작성일: {{ answer.resdate }}</p>\r\n          <button @click=\"goToAnswerUpdate(answer.qno)\">답변 수정</button>\r\n        </div>\r\n      </div>\r\n      <div v-else>\r\n        <button @click=\"goToAnswerInsert\">답변 등록</button>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { mapState, mapActions } from 'vuex';\r\n  \r\n  export default {\r\n    computed: {\r\n      ...mapState(['qna', 'answers']),\r\n      isQuestion() {\r\n        return this.qna.lev === 0;\r\n      },\r\n    },\r\n    methods: {\r\n      ...mapActions(['fetchQna', 'fetchAnswers', 'deleteQna']),\r\n      goToUpdate() {\r\n        this.$router.push({ name: 'QnaUpdate', params: { id: this.qna.qno } });\r\n      },\r\n      deleteQna() {\r\n        this.deleteQna(this.qna.qno).then(() => {\r\n          this.$router.push({ name: 'QnaList' });\r\n        });\r\n      },\r\n      goToAnswerInsert() {\r\n        this.$router.push({ name: 'AnswerInsert', params: { id: this.qna.qno } });\r\n      },\r\n      goToAnswerUpdate(answerId) {\r\n        this.$router.push({ name: 'QnaUpdate', params: { id: answerId } });\r\n      },\r\n    },\r\n    created() {\r\n      const qno = this.$route.params.id;\r\n      this.fetchQna(qno);\r\n      this.fetchAnswers(qno);\r\n    },\r\n  };\r\n  </script>\r\n  "],"mappings":";AAuBE,SAASA,QAAQ,EAAEC,UAAS,QAAS,MAAM;AAE3C,eAAe;EACbC,QAAQ,EAAE;IACR,GAAGF,QAAQ,CAAC,CAAC,KAAK,EAAE,SAAS,CAAC,CAAC;IAC/BG,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACC,GAAG,CAACC,GAAE,KAAM,CAAC;IAC3B;EACF,CAAC;EACDC,OAAO,EAAE;IACP,GAAGL,UAAU,CAAC,CAAC,UAAU,EAAE,cAAc,EAAE,WAAW,CAAC,CAAC;IACxDM,UAAUA,CAAA,EAAG;MACX,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE,WAAW;QAAEC,MAAM,EAAE;UAAEC,EAAE,EAAE,IAAI,CAACR,GAAG,CAACS;QAAI;MAAE,CAAC,CAAC;IACxE,CAAC;IACDC,SAASA,CAAA,EAAG;MACV,IAAI,CAACA,SAAS,CAAC,IAAI,CAACV,GAAG,CAACS,GAAG,CAAC,CAACE,IAAI,CAAC,MAAM;QACtC,IAAI,CAACP,OAAO,CAACC,IAAI,CAAC;UAAEC,IAAI,EAAE;QAAU,CAAC,CAAC;MACxC,CAAC,CAAC;IACJ,CAAC;IACDM,gBAAgBA,CAAA,EAAG;M